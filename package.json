{
  "name": "integrail-sdk-cloud",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "scripts": {
    "check": "tspc --noEmit",
    "check:watch": "tspc --noEmit --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts' --fix --quiet",
    "prepare": "npm run build",
    "build": "rimraf dist && tspc -p ./tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "exports": {
    ".": {
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "./api": {
      "require": {
        "default": "./dist/api/integrail.api.js",
        "types": "./dist/api/integrail.api.d.ts"
      },
      "import": {
        "default": "./dist/api/integrail.api.js",
        "types": "./dist/api/integrail.api.d.ts"
      }
    },
    "./prelude": {
      "require": {
        "default": "./dist/prelude/index.js",
        "types": "./dist/prelude/index.d.ts"
      },
      "import": {
        "default": "./dist/prelude/index.js",
        "types": "./dist/prelude/index.d.ts"
      }
    },
    "./types": {
      "require": {
        "default": "./dist/types/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "import": {
        "default": "./dist/types/index.js",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@jest/globals": "^29.7.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "json-schema-to-typescript": "^14.1.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.2",
    "typescript-transform-paths": "^3.4.7",
    "zod-to-json-schema": "^3.23.1"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.6",
    "ajv": "^7.2.4",
    "fp-ts": "^2.16.9",
    "node-abort-controller": "^3.1.1",
    "openapi3-ts": "^4.3.3",
    "reflect-metadata": "^0.2.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "volta": {
    "node": "14.21.3"
  }
}
